<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="questionService" class="com.example.student_testing.service.QuestionsServiceImpl">
        <property name="fileName" value="test.csv"/>
    </bean>

    <bean id="studentTestingService" class="com.example.student_testing.service.StudentTestingServiceImpl">
        <constructor-arg name="qService" ref="questionService"/>
    </bean>
</beans>
